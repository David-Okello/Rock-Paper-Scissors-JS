{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "public/game.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/game.css",
      "use": "@vercel/static"
    },
    {
      "src": "public/game.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/game.html"
    },
    {
      "src": "/game.css",
      "dest": "/public/game.css"
    },
    {
      "src": "/game.js",
      "dest": "/public/game.js"
    }
  ]
}
